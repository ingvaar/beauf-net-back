{
	"name": "beauf-net-api",
	"version": "0.0.1",
	"description": "A quote API",
	"author": "Ingvaar",
	"private": true,
	"license": "MIT",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug 0.0.0.0:9229 --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
		"test": "jest --runInBand",
		"test:watch": "jest --watch --runInBand",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest",
		"test:e2e": "jest --config ./test/jest-e2e.json --forceExit",
		"typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./ormconfig-migrations.ts",
		"migrate": "yarn run typeorm:cli migration:generate -n",
		"premigrate": "yarn build"
	},
	"dependencies": {
		"@nestjs-modules/mailer": "^1.8.1",
		"@nestjs/axios": "^1.0.1",
		"@nestjs/common": "^9.4.2",
		"@nestjs/config": "^2.3.2",
		"@nestjs/core": "^9.4.2",
		"@nestjs/jwt": "^10.1.0",
		"@nestjs/passport": "^9.0.3",
		"@nestjs/platform-express": "^9.4.3",
		"@nestjs/swagger": "^6.3.0",
		"@nestjs/typeorm": "^9.0.1",
		"bcrypt": "^5.1.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"ejs": "^3.1.9",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"express": "^4.18.2",
		"handlebars": "^4.7.7",
		"morgan": "^1.10.0",
		"nodemailer": "^6.9.3",
		"passport": "^0.6.0",
		"passport-jwt": "^4.0.1",
		"passport-local": "^1.0.0",
		"pg": "^8.11.1",
		"pug": "^3.0.2",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.8.1",
		"supertest": "^6.3.3",
		"typeorm": "^0.3.17",
		"uuid": "^9.0.0",
		"webpack": "^5.88.1"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.1.8",
		"@nestjs/testing": "^9.4.2",
		"@types/bcrypt": "^5.0.0",
		"@types/ejs": "^3.1.2",
		"@types/inline-css": "^3.0.1",
		"@types/jest": "^27.4.0",
		"@types/morgan": "^1.9.4",
		"@types/node": "^20.4.2",
		"@types/nodemailer": "^6.4.8",
		"@types/passport-jwt": "^3.0.9",
		"@types/passport-local": "^1.0.35",
		"@types/supertest": "^2.0.12",
		"@types/uuid": "^9.0.2",
		"@types/pug": "^2.0.6",
		"@types/validator": "^13.7.17",
		"@typescript-eslint/eslint-plugin": "5.62.0",
		"@typescript-eslint/parser": "5.61.0",
		"eslint": "8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"jest": "27.5.1",
		"prettier": "^2.8.8",
		"ts-jest": "27.1.3",
		"ts-loader": "^9.4.3",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^4.7.4"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	},
	"repository": "git@github.com:ingvaar/beauf-net-back.git"
}
