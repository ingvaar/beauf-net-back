{
	"name": "beauf-net-api",
	"version": "0.0.1",
	"description": "A quote API",
	"author": "Ingvaar",
	"private": true,
	"license": "MIT",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug 0.0.0.0:9229 --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
		"test": "jest --runInBand",
		"test:watch": "jest --watch --runInBand",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest",
		"test:e2e": "jest --config ./test/jest-e2e.json --forceExit",
		"typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./ormconfig-migrations.ts",
		"migrate": "yarn run typeorm:cli migration:generate -n",
		"premigrate": "yarn build"
	},
	"dependencies": {
		"@nestjs-modules/mailer": "^1.6.1",
		"@nestjs/axios": "^0.0.7",
		"@nestjs/common": "^8.4.4",
		"@nestjs/config": "^2.0.0",
		"@nestjs/core": "^8.4.5",
		"@nestjs/jwt": "^8.0.0",
		"@nestjs/passport": "^8.2.1",
		"@nestjs/platform-express": "^8.4.4",
		"@nestjs/swagger": "^5.1.5",
		"@nestjs/typeorm": "^8.0.3",
		"bcrypt": "^5.0.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"express": "^4.17.2",
		"handlebars": "^4.7.7",
		"morgan": "^1.10.0",
		"nodemailer": "^6.7.2",
		"passport": "^0.5.2",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"pg": "^8.7.1",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.5.2",
		"supertest": "^6.2.2",
		"swagger-ui-express": "^4.3.0",
		"typeorm": "^0.2.41",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@nestjs/cli": "^8.2.5",
		"@nestjs/testing": "^8.4.3",
		"@types/bcrypt": "^5.0.0",
		"@types/inline-css": "^3.0.1",
		"@types/jest": "^27.4.0",
		"@types/morgan": "^1.9.3",
		"@types/node": "^17.0.26",
		"@types/nodemailer": "^6.4.4",
		"@types/passport-jwt": "^3.0.3",
		"@types/passport-local": "^1.0.33",
		"@types/supertest": "^2.0.11",
		"@types/uuid": "^8.3.4",
		"@types/validator": "^13.7.2",
		"@typescript-eslint/eslint-plugin": "5.18.0",
		"@typescript-eslint/parser": "5.20.0",
		"eslint": "8.14.0",
		"eslint-config-prettier": "^8.5.0",
		"jest": "27.5.1",
		"prettier": "^2.5.1",
		"ts-jest": "27.1.3",
		"ts-loader": "^9.2.8",
		"ts-node": "^10.7.0",
		"tsconfig-paths": "^3.14.1",
		"typescript": "^4.6.3"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	},
	"repository": "git@github.com:ingvaar/beauf-net-back.git"
}
